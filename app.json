{
  "name": "mozgbrasil/php-labs",
  "description": "php labs",
  "keywords": ["heroku", "mozg", "brasil"],
  "website": "http://mozg.com.br/",
  "repository": "https://github.com/mozgbrasil/php-labs",
  "logo": "http://mozg.com.br/assets/images/logos/logo.svg",
  "success_url": "/",
  "scripts": {
    "postdeploy": "./app.sh app_scripts_postdeploy",
    "pr-predestroy": "./app.sh app_scripts_predestroy"
  },
  "env": {
    "LANG": {
      "description": "https://devcenter.heroku.com/articles/buildpack-api",
      "value": "en_US.UTF-8"
    },
    "SECRET_TOKEN": {
      "description": "A secret key for verifying the integrity of signed cookies.",
      "generator": "secret"
    },
      "APP_ENV": {
            "description": "-",
            "value": "local",
            "required": false
        },
        "APP_KEY": {
            "description": "Digite o resultado do pr√≥ximo comando 'php artisan --no-ansi key:generate --show'",
            "required": true
        },
        "APP_DEBUG": {
            "description": "-",
            "value": "true",
            "required": false
        },
        "APP_URL": {
            "description": "-",
            "value": "https://heroku-laravel-mozg.herokuapp.com/",
            "required": true
        },
        "LOG_CHANNEL": {
            "description": "-",
            "value": "stack",
            "required": true
        },
        "DB_CONNECTION": {
            "description": "-",
            "value": "pgsql",
            "required": true
        },
        "DB_HOST": {
            "description": "-",
            "value": "127.0.0.1",
            "required": true
        },
        "DB_PORT": {
            "description": "-",
            "value": "5432",
            "required": true
        },
        "DB_DATABASE": {
            "description": "-",
            "value": "homestead",
            "required": true
        },
        "DB_USERNAME": {
            "description": "-",
            "value": "root",
            "required": true
        },
        "DB_PASSWORD": {
            "description": "-",
            "value": "secret",
            "required": true
        },
        "BROADCAST_DRIVER": {
            "description": "-",
            "value": "log",
            "required": true
        },
        "CACHE_DRIVER": {
            "description": "-",
            "value": "file",
            "required": true
        },
        "QUEUE_CONNECTION": {
            "description": "-",
            "value": "sync",
            "required": true
        },
        "SESSION_DRIVER": {
            "description": "-",
            "value": "file",
            "required": true
        },
        "SESSION_LIFETIME": {
            "description": "-",
            "value": "120",
            "required": true
        },
        "REDIS_HOST": {
            "description": "-",
            "value": "127.0.0.1",
            "required": true
        },
        "REDIS_PORT": {
            "description": "-",
            "value": "6379",
            "required": true
        },
        "REDIS_USER": {
            "description": "-",
            "value": "null",
            "required": true
        },
        "REDIS_PASSWORD": {
            "description": "-",
            "value": "null",
            "required": true
        },
        "EMAIL":{
            "description": "-",
            "value": "",
            "required":true
        }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "free"
    },
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
  "addons": [],
  "buildpacks": [
    {
      "url": "heroku/php"
    }
  ],
  "environments": {
    "test": {
      "scripts": {
        "test": "./app.sh app_environments_test"
      }
    }
  }
}
